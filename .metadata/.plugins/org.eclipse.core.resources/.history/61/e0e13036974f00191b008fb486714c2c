package branching;

public class BreakStament {

	public void unlableBreakStament() {
		int in = 0;
		int out = 0;
		for (out = 0; out < 10; out++) {
			for (in = 0; in < 20; in++) {
				if (in > 10)
					break;
			}
			System.out.println("inside the outer loop" + " out: " + out + " in: " + in);
		}
		System.out.println("outside the outer loop" + " out: " + out + " in: " + in);
	}
}
